# Define the data
months <- c("Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec")
ozone_levels <- c(30, 45, 60, 70, 90, 110, 130, 120, 80, 60, 40, 30)

# Create the Bar Plot
barplot(ozone_levels, names.arg = months, col = "skyblue", 
        main = "Monthly Ozone Concentration in Air",
        xlab = "Month", ylab = "Ozone Concentration (ppb)",
        ylim = c(0, max(ozone_levels) + 20))  # Adjust y-axis limits for data labels

